    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kevinfrom\\EconomicApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kevinfrom\\EconomicApi\\Tests\\": "tests/"
